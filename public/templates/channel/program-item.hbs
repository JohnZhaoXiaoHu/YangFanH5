{{#if Schedules.Schedule}}
    {{#each Schedules.Schedule}}
        <li>
            <span class="date">{{StartTime}}</span>
            <span class="name overflow">{{ProgramName}}</span>
            {{#transformat PlayState}}
                <span id="play" class="play">正在播放</span>
            {{/transformat}}
        </li>
    {{/each}}
{{else}}
    <p>暂时没有数据！</p>
{{/if}}